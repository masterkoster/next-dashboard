{
  "globalHeaders": {
    "X-Frame-Options": "DENY"
  },
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": ["/*.{png,jpg,jpeg,gif,svg,ico,woff2,css,js,json}"]
  },
  "routes": [
    {
      "route": "/api/*",
      "allowedRoles": ["anonymous"]
    }
  ],
  "mimeTypes": {
    ".json": "text/json"
  }
}
